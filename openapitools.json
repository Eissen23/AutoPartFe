{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:5026/swagger/v1/swagger.json",
        "output": "src/openapi",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        },
        "typeMappings": {
          "set": "Array"
        }
      }
    }
  }
}
